{
	"presets": [
		[ "latest", {
			"es2015": {
				"modules": false
			}
		} ]
	],
	"plugins": [
		"lodash",
		"transform-object-rest-spread",
		[ "transform-react-jsx", {
			"pragma": "wp.element.createElement"
		} ]
	],
	"env": {
		"default": {
			"plugins": [
				"transform-runtime"
			]
		},
		"test": {
			"presets": [ "latest" ]
		},
		"gettext": {
			"plugins": [
				[ "./i18n/babel-plugin", {
					"output": "languages/gutenberg.pot"
				} ]
			]
		}
	}
}
