{
	"root": true,
	"extends": [
		"plugin:@wordpress/eslint-plugin/es5"
	],
	"env": {
		"node": true
	},
	"settings": {
		"jsdoc": {
			"mode": "typescript"
		}
	},
	"overrides": [
		{
			"files": [ "@(test|benchmark)/*.js" ],
			"extends": [
				"plugin:@wordpress/eslint-plugin/recommended"
			]
		}
	]
}
